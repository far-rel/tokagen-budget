{
  "name": "InSight",
  "description": "InSIght",
  "scripts": {
    "test": "jest app/assets/javascripts/__spec__/",
    "postinstall": "gulp build --production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tokagen/insight.git"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "babelify": "~6.3.0",
    "browserify": "~11.1.0",
    "classnames": "~2.1.3",
    "counterpart": "~0.16.10",
    "del": "~1.1.1",
    "gulp": "~3.9.0",
    "gulp-autoprefixer": "~2.1.0",
    "gulp-concat": "~2.5.2",
    "gulp-connect": "~2.2.0",
    "gulp-eslint": "~1.0.0",
    "gulp-livereload": "~3.8.0",
    "gulp-load-plugins": "~0.9.0",
    "gulp-notify": "~1.4.2",
    "gulp-size": "~1.2.1",
    "gulp-uglify": "~1.4.1",
    "gulp-util": "~3.0.4",
    "gulp-watch": "~4.3.5",
    "harmonize": "~1.4.4",
    "lodash": "~2.4.1",
    "moment": "~2.10.6",
    "pretty-hrtime": "~1.0.0",
    "qwest": "~4.0.0",
    "react": "~0.14.7",
    "react-dom": "~0.14.7",
    "react-addons-pure-render-mixin": "~0.14.7",
    "react-addons-linked-state-mixin": "~0.14.7",
    "react-router": "~2.0.0",
    "react-cookie": "~0.4.7",
    "react-date-picker": "~4.0.10",
    "require-dir": "~0.1.0",
    "underscore": "~1.8.3",
    "underscore.string": "~3.2.2",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~2.2.1",
    "redux": "~3.3.1",
    "react-redux": "~4.4.0",
    "react-router-redux": "~4.0.0",
    "node-uuid": "~1.4.7",
    "redux-thunk": "~2.0.1",
    "redux-logger": "~2.6.1"
  },
  "engine": {
    "node": "0.10.x"
  },
  "jest": {
    "rootDir": "./app/assets/javascripts",
    "scriptPreprocessor": "../../../node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "react",
      "es6"
    ],
    "testDirectoryName": "__spec__",
    "unmockedModulePathPatterns": [
      "react",
      "__spec_helpers__"
    ],
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "node_modules",
      "__spec_helpers__"
    ]
  },
  "devDependencies": {
    "react-addons-test-utils": "~0.14.7",
    "eslint-plugin-react": "~3.4.1",
    "jest-cli": "~0.4.0",
    "gulp-jest": "~0.4.0",
    "babel-eslint": "~4.1.6",
    "babel-jest": "~5.3.0",
    "eslint": "~1.4.3"
  }
}
